{
  "name": "seneca-external-ip-check",
  "version": "0.0.0-semantically-released",
  "description": "Seneca plugin to return the external IP of server",
  "main": "index.js",
  "keywords": [
    "seneca",
    "senecaplugin",
    "seneca-plugin",
    "external",
    "ipv4",
    "vpn"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/boneskull/seneca-external-ip-check.git"
  },
  "author": "Christopher Hiller <boneskull@boneskull.com> (https://boneskull.com/)",
  "license": "MIT",
  "dependencies": {
    "external-ip": "0.2.4"
  },
  "peerDependencies": {
    "seneca": "^3.0.0"
  },
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "condition-node-version": "^1.3.0",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.5.0",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.0.1",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "ghooks": "^1.3.2",
    "mocha": "^3.0.2",
    "nsp": "^2.6.1",
    "nyc": "^8.3.0",
    "opt-cli": "^1.5.1",
    "p-s": "^3.0.1",
    "semantic-release": "^4.3.5",
    "seneca": "^3.1.0",
    "validate-commit-msg": "^2.8.0"
  },
  "scripts": {
    "start": "p-s",
    "test": "p-s test"
  },
  "release": {
    "verifyConditions": [
      {
        "path": "condition-node-version",
        "node": "6",
        "verbose": true
      },
      "@semantic-release/condition-travis"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "opt --out pre-commit --exec \"npm test\"",
      "commit-msg": "opt --out commit-msg --exec validate-commit-msg"
    }
  }
}
